#include "mt7621.dtsi"
#include <dt-bindings/input/input.h>

/ {
	compatible = "rfb-ax5400p-nor";
	model = "MediaTek MT7621 RFB (802.11ax,SNOR)";

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	keys {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};
};

&spi0 {
	status = "okay";

	flash@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "FM25Q128","jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <50000000>;
		
        partitions {
            compatible = "fixed-partitions";
            #address-cells = <1>;
            #size-cells = <1>;
			partition@0 {
				label = "Bootloader";
				reg = <0x00000 0x30000>;
				read-only;
			};

			partition@30000 {
				label = "Config";
				reg = <0x30000 0x10000>;
				read-only;
			};

			factory:partition@50000 {
				label = "Factory";
				reg = <0x50000 0x40000>;
				read-only;
			};

			partition@90000 {
				compatible = "denx,fit";
				label = "firmware";
				reg = <0x90000 0xf70000>;
			};
		};
	};
};

&state_default {
	gpio {
		groups = "uart2", "uart3";
		function = "gpio";
	};
};

&sdhci {
	status = "okay";
};

&xhci {
	status = "disabled";
};

&pcie {
	status = "okay";
};

&pcie1 {
	wifi@0,0 {
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x0000>;
	};
};

&gmac0 {
	nvmem-cells = <&macaddr_factory_4>;
	nvmem-cell-names = "mac-address";
	mac-address-increment = <2>;
};

&switch0 {
	ports {
		wan_port: port@0 {
			status = "okay";
			label = "wan";
			nvmem-cells = <&macaddr_factory_4>;
			nvmem-cell-names = "mac-address";
			mac-address-increment = <3>;
		};
		port@1 {
			status = "okay";
			label = "lan1";
		};
		port@2 {
			status = "okay";
			label = "lan2";
		};
		port@4 {
			status = "okay";
			label = "lan3";
		};
	};
};

&factory {
	compatible = "nvmem-cells";
	#address-cells = <1>;
	#size-cells = <1>;

	macaddr_factory_4: macaddr@4 {
		reg = <0x4 0x6>;
	};
};